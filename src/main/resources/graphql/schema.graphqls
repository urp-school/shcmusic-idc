type Student{
  id : ID!
  code:String!
  name:String!
  gradeCode:String!
  eduTypeName:String!
  levelName:String!
  departName:String!
  majorName:String!
  directionName:String
  duration:Float!
  stdTypeName:String!
  studyTypeName:String
  statusName:String!
  advisorName:String
  genderName:String!
